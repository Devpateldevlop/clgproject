{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/product",
      "dest": "/api/fetproduct.js" 
    },
    {
      "src": "/api/listed",
      "dest": "/api/listedprod.js" 
    },
    {
      "src": "/api/upload",
      "dest": "/api/listedprod.js" 
    },
    {
      "src": "/api/login",
      "dest": "/api/login.js" 
    }
    ]
}
